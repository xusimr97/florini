{
  "name": "Payment",
  "plural": "Payments",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "paymentDate": {
      "type": "date",
      "required": true
    },
    "paymentMethodId": {
      "type": "string",
      "required": true
    },
    "orderId": {
      "type": "string",
      "required": true
    },
    "amount": {
      "type": "number",
      "required": true
    },
    "street": {
      "type": "string",
      "required": true
    },
    "number": {
      "type": "number"
    },
    "zipCode": {
      "type": "number",
      "required": true
    },
    "additionalInformation": {
      "type": "string"
    },
    "city": {
      "type": "string",
      "required": true
    },
    "regionId": {
      "type": "string",
      "required": true
    },
    "countryId": {
      "type": "string",
      "required": true
    },
    "state": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "country": {
      "type": "belongsTo",
      "model": "Country",
      "foreignKey": "countryId",
      "options": {
        "nestRemoting": true
      }
    },
    "paymentMethod": {
      "type": "belongsTo",
      "model": "PaymentMethod",
      "foreignKey": "paymentMethodId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
