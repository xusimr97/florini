{
  "name": "TagTranslation",
  "plural": "TagTranslations",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "locale": {
      "type": "string",
      "required": true,
      "default": "es"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "tagId": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "tag": {
      "type": "belongsTo",
      "model": "Tag",
      "foreignKey": "tagId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
